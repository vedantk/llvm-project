#ifndef LLDB_STATISTIC_DEF
#error "Must define LLDB_STATISTIC_DEF(EnumName, Desc)."
#endif

LLDB_STATISTIC_DEF(ExpressionSuccess, "Number of expr evaluation successes")
LLDB_STATISTIC_DEF(ExpressionFailure, "Number of expr evaluation failures")
LLDB_STATISTIC_DEF(FrameVarSuccess, "Number of frame var successes")
LLDB_STATISTIC_DEF(FrameVarFailure, "Number of frame var failures")
LLDB_STATISTIC_DEF(AssertionFailure, "Number of lldbassert() failures")

#undef LLDB_STATISTIC_DEF
